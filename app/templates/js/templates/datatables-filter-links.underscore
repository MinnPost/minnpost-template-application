<div class="datatables-filter-links-container">
  <div class="filter-label">Filters:</div>

  <ul class="datatables-filter-links cf">
    <%% _.each(options, function(o, oi) { %>
      <li><a class="filter-link" data-col="<%%= o.col %>" href="#<%%= o.term %>"><%%= o.term %></a></li>
    <%% }) %>
    <li><a class="filter-link filter-clear" href="#clear">Show all</a></li>
  </ul>
</div>
